<div class="instagram-post instagram-post-{{ post.type | clean_class }}">
  <!-- /.instagram-post -->
  {% if post.media_url %}
    {% if post.post_url %}
      <a href="{{ post.post_url }}">
        <img src="{{ post.media_url }}" />
      </a>
    {% else %}
      <img src="{{ post.media_url }}" />
    {% endif %}
  {% endif %}
</div>
